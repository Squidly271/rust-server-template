  <DonateText/>
  <DonateLink/>
  <Description>Fully customizable Rust Server at your fingertips. [br] &#xD;
Set all of the settings below to suit your needs and save when you are ready. [br]  [br]&#xD;
*  Is a requirement that Rust needs to startup. &#xD;
&#xD;
 &#xD;
</Description>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>28015</HostPort>
        <ContainerPort>28015</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>28016</HostPort>
        <ContainerPort>28016</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>28015</HostPort>
        <ContainerPort>28015</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/Rust-Server/</HostDir>
      <ContainerDir>/steamcmd/rust</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value/>
      <Name>RUST_SERVER_STARTUP_ARGUMENTS</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>My-server-file</Value>
      <Name>RUST_SERVER_IDENTITY</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>0</Value>
      <Name>RUST_SERVER_SEED</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>My server name</Value>
      <Name>RUST_SERVER_NAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>RUST_SERVER_DESCRIPTION</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>changeme</Value>
      <Name>RUST_RCON_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>1</Value>
      <Name>RUST_UPDATE_CHECKING</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>public</Value>
      <Name>RUST_UPDATE_BRANCH</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>0</Value>
      <Name>RUST_OXIDE_ENABLED</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>1</Value>
      <Name>RUST_OXIDE_UPDATE_ON_BOOT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>3500</Value>
      <Name>RUST_SERVER_WORLDSIZE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>200</Value>
      <Name>RUST_SERVER_MAXPLAYERS</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>300</Value>
      <Name>RUST_SERVER_SAVE_INTERVAL</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>1</Value>
      <Name>RUST_RCON_WEB</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>RUST_SERVER_BANNER_URL</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>STEAMUSER</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>STEAMPW</Name>
      <Mode/>
    </Variable>

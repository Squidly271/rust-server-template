  <Labels/>
  <Config Name="Rust Arguments" Target="RUST_SERVER_STARTUP_ARGUMENTS" Default="" Mode="" Description="Arguments on startup" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Server Identity " Target="RUST_SERVER_IDENTITY" Default="" Mode="" Description="Name the server folder will be titled.  No spaces" Type="Variable" Display="always" Required="true" Mask="false">My-server-file</Config>
  <Config Name="Server Seed" Target="RUST_SERVER_SEED" Default="" Mode="" Description="Map Seed is what shapes your make the way you want it. &amp;lt;/br&amp;gt;&#13;&#10;Use online Rust seed generator for your server.  &amp;lt;/br&amp;gt;&#13;&#10;0 = will randomize the seed each time the map is deleted. " Type="Variable" Display="always" Required="true" Mask="false">0</Config>
  <Config Name="Server Name" Target="RUST_SERVER_NAME" Default="" Mode="" Description="Server name that will appear on server listing" Type="Variable" Display="always" Required="true" Mask="false">My server name</Config>
  <Config Name="Server Description" Target="RUST_SERVER_DESCRIPTION" Default="" Mode="" Description="Server Description that will show up on server listing" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Rcon Password" Target="RUST_RCON_PASSWORD" Default="" Mode="" Description="Set a password to remote access the server" Type="Variable" Display="always" Required="false" Mask="false">changeme</Config>
  <Config Name="Auto Update" Target="RUST_UPDATE_CHECKING" Default="" Mode="" Description="0 = Disabled &amp;lt;/br&amp;gt;&#13;&#10;1 = Enabled" Type="Variable" Display="always" Required="true" Mask="false">1</Config>
  <Config Name="Server Type" Target="RUST_UPDATE_BRANCH" Default="public" Mode="" Description="public &amp;lt;/br&amp;gt;&#13;&#10;prerelease" Type="Variable" Display="always" Required="true" Mask="false">public</Config>
  <Config Name="Oxide Enabled" Target="RUST_OXIDE_ENABLED" Default="0" Mode="" Description="0 = Disabled &amp;lt;/br&amp;gt;&#13;&#10;1 = Enabled &amp;lt;/br&amp;gt;&#13;&#10;This is if you want to use plugins on the server.&#13;&#10;" Type="Variable" Display="always" Required="true" Mask="false">0</Config>
  <Config Name="Update Oxide On Boot" Target="RUST_OXIDE_UPDATE_ON_BOOT" Default="" Mode="" Description="0 = Disabled &amp;lt;/br&amp;gt;&#13;&#10;1 = Enabled" Type="Variable" Display="always" Required="true" Mask="false">1</Config>
  <Config Name="World Size" Target="RUST_SERVER_WORLDSIZE" Default="3500" Mode="" Description="The size of the map that will be generate. " Type="Variable" Display="always" Required="true" Mask="false">3500</Config>
  <Config Name="Max Players" Target="RUST_SERVER_MAXPLAYERS" Default="" Mode="" Description="Max amount of players that can play at once" Type="Variable" Display="always" Required="true" Mask="false">200</Config>
  <Config Name="Server Save Intervals" Target="RUST_SERVER_SAVE_INTERVAL" Default="300" Mode="" Description="The time it take in seconds to save world data" Type="Variable" Display="always" Required="true" Mask="false">300</Config>
  <Config Name="Rcon Web Enabled" Target="RUST_RCON_WEB" Default="1" Mode="" Description="0 = Disabled &amp;lt;/br&amp;gt;&#13;&#10;1 = Enabled" Type="Variable" Display="always" Required="true" Mask="false">1</Config>
  <Config Name="Server Banner URL" Target="RUST_SERVER_BANNER_URL" Default="" Mode="" Description="Must be 512x256 and PNG file" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Steam Username" Target="STEAMUSER" Default="" Mode="" Description="Leave blank for anonymous login. &amp;lt;/br&amp;gt;&#13;&#10;Steam username (Cap Sensitive) " Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Steam Password" Target="STEAMPW" Default="" Mode="" Description="Enter a steam password if Steam Username has been entered. " Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Secure Server" Target="RUST_SERVER_SECURE" Default="1" Mode="" Description="1 = Enabled &amp;lt;/br&amp;gt;&#13;&#10;0 = Disabled&#13;&#10;" Type="Variable" Display="always" Required="true" Mask="false">1</Config>
  <Config Name="Server FPS" Target="RUST_SERVER_FPS" Default="-1" Mode="" Description="Server FPS Settings.  &amp;lt;/br&amp;gt;&#13;&#10;-1 = No limit" Type="Variable" Display="always" Required="true" Mask="false">-1</Config>
  <Config Name="Server Batch Update" Target="RUST_SERVER_UPDATEBATCH" Default="256" Mode="" Description="This value changes the update rate of objects/players in-game." Type="Variable" Display="always" Required="false" Mask="false">256</Config>
  <Config Name="Auto Port Forwarding" Target="UPNP" Default="1" Mode="" Description="1 = Enabled &amp;lt;/br&amp;gt;&#13;&#10;0 = Disabled &amp;lt;/br&amp;gt;&#13;&#10;Turn this feature on to automatically open your ports on your router to access the game outside your network. &amp;lt;/br&amp;gt;&#13;&#10;NOTE: This only works with routers that support (UPnP) " Type="Variable" Display="always" Required="true" Mask="false">1</Config>
  <Config Name="Server Port1" Target="28015" Default="" Mode="udp" Description="Container Port: " Type="Port" Display="advanced" Required="false" Mask="false">28015</Config>
  <Config Name="Rcon Port" Target="28016" Default="" Mode="tcp" Description="Container Port: " Type="Port" Display="advanced" Required="false" Mask="false">28016</Config>
  <Config Name="Server Port2" Target="28015" Default="" Mode="tcp" Description="Container Port: " Type="Port" Display="advanced" Required="false" Mask="false">28015</Config>
  <Config Name="Rust Directory" Target="/steamcmd/rust" Default="/mnt/cache/appdata/Rust-Server/" Mode="rw" Description="Location we're you want the Rust directory to be saved for easier access. " Type="Path" Display="advanced" Required="true" Mask="false">/mnt/cache/appdata/Rust-Server/</Config>
  <Config Name="Web Port" Target="8080" Default="8080" Mode="tcp" Description="Web host for Rcon" Type="Port" Display="advanced" Required="false" Mask="false">8080</Config>
</Container>
